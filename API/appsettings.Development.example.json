{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "ConnectionStrings": {
    "PostgreSQLLocal": "Host=[DataBaseHostName]:[DataBaseHostPortNumber];Database=[DataBaseName];Username=[DataBaseUserName];Password=[DataBasePassword]",
    "IdentityConnection": "Host=[DataBaseHostName]:[DataBaseHostPortNumber];Database=[DataBaseName];Username=[DataBaseUserName];Password=[DataBasePassword]",
    "Redis": "[RedisHostName]"
  },
  "Token": {
    "Key": "[TokenKey]",
    "Issuer": "[TokenIssuerHostAddress]"
  },
  "ApiUrl": "[ApiUrlHostAddress]"
}
